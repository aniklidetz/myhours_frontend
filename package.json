{
  "name": "myhours-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --testPathIgnorePatterns=/e2e/",
    "test:unit": "jest --watchAll=false --testPathIgnorePatterns=/e2e/",
    "test:security": "jest src/utils/__tests__/safeLogging.test.js --verbose",
    "test:security-ci": "jest src/utils/__tests__/safeLogging.test.js --ci --coverage --collectCoverageFrom='src/utils/safeLogging.js'",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:auth": "playwright test auth.spec.js",
    "test:e2e:security": "playwright test security.spec.js",
    "lint": "eslint \"app/**/*.{js,jsx}\" \"src/**/*.{js,jsx}\" \"components/**/*.{js,jsx}\" \"contexts/**/*.{js,jsx}\" \"hooks/**/*.{js,jsx}\" \"utils/**/*.{js,jsx}\" --max-warnings 50",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/__tests__/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).js",
      "**/?(*.)+(test|spec).js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/",
      "jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/**/__tests__/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo/config-plugins": "^10.0.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.1",
    "@react-navigation/stack": "^7.2.5",
    "axios": "^1.9.0",
    "expo": "53.0.19",
    "expo-blur": "~14.0.1",
    "expo-camera": "~16.0.6",
    "expo-constants": "~17.0.2",
    "expo-device": "~7.0.1",
    "expo-font": "~13.0.1",
    "expo-haptics": "~14.0.0",
    "expo-linear-gradient": "~14.0.0",
    "expo-linking": "~7.0.3",
    "expo-location": "~18.0.2",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.30.1",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.4.0",
    "expo-system-ui": "~5.0.1",
    "expo-web-browser": "~14.0.1",
    "metro": "^0.82.0",
    "metro-config": "^0.82.0",
    "metro-resolver": "^0.82.0",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.27.0",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.55.0",
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "playwright": "^1.55.0",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "private": true
}
